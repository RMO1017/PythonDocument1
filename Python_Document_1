    "##### format()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 71,
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "what is your age?  42\n",
      "what is your name?  harper\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "You are harper, you are 42 years old and you are getting wiser.\n"
     ]
    }
   ],
   "source": [
    "age = input(\"what is your age? \")\n",
    "name = input(\"what is your name? \")\n",
    "\n",
    "result_string = \"You are {}, you are {} years old and you are getting wiser.\".format(name, age)\n",
    "print(result_string)"
   ]
  },
  {
